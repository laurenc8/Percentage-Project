,"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/pages/PageData.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/PhotoGallery.js",["74","75"],"import { useState } from 'react';\nimport \"./PhotoGallery.css\";\nimport { features } from '../constants/features';\n\n//MAIN APP COMPONENT\nexport default function Gallery() {\n  return (\n    <div className=\"Gallery\">\n      <ImageGallery />\n    </div>\n  );\n}\n\nconst images = Object.values(features).filter(person => person.photostat);\n\n//MAIN LIGHTBOX\n//Holds Images Cards and Lightbox\n//this is where all of our logic will live\nfunction ImageGallery() {\n  const [imageToShow, setImageToShow] = useState(\"\");\n  const [lightboxDisplay, setLightBoxDisplay] = useState(false);\n  \n  //looping through our images array to create img elements\n  const imageCards = images.map((image) => (\n    <img className=\"image-card\" onClick={() => showImage(image)} src={image.photostat}alt=\"\" />\n  ));\n\n  //function to show a specific image in the lightbox, amd make lightbox visible\n  const showImage = (image) => {\n    setImageToShow(image);\n    setLightBoxDisplay(true);\n    disableScroll();\n  };\n\n  //hide lightbox\n  const hideLightBox = () => {\n    setLightBoxDisplay(false);\n    enableScroll();\n  };\n\n  //show next image in lightbox\n  const showNext = (e) => {\n    e.stopPropagation();\n    let currentIndex = images.indexOf(imageToShow);\n    if (currentIndex >= images.length - 1) {\n      setLightBoxDisplay(false);\n    } else {\n      let nextImage = images[currentIndex + 1];\n      setImageToShow(nextImage);\n    }\n  };\n\n  //show previous image in lightbox\n  const showPrev = (e) => {\n    e.stopPropagation();\n    let currentIndex = images.indexOf(imageToShow);\n    if (currentIndex <= 0) {\n      setLightBoxDisplay(false);\n    } else {\n      let nextImage = images[currentIndex - 1];\n      setImageToShow(nextImage);\n    }\n  };\n\n  function disableScroll() {\n    document.body.style.overflow = 'hidden';\n    document.querySelector('html').scrollTop = window.scrollY;\n    }\n    \n  function enableScroll() {\n    document.body.style.overflow = null;\n    }\n\n  return (\n    <>\n      <div> {imageCards} </div>\n      \n      {\n        lightboxDisplay &&\n        <div id=\"lightbox\" onClick={hideLightBox}>\n          <a class=\"prev\" onClick={showPrev}>&#10094;</a>\n          <img id=\"lightbox-img\" src={imageToShow.photostat} alt = \"\"></img>\n          <a class=\"next\" onClick={showNext}>&#10095;</a>\n        </div>\n      }\n    </>\n  );\n}\n","/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/constants/data.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/constants/features.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/StoriesBars.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/Container.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/Dots.js",["76","77","78","79"],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/LineGraph.js",["80","81","82","83","84","85","86","87","88","89","90","91","92","93","94","95","96","97","98"],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/styles/CustomChartBackground.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/constants/concentration_data.js",[],"/Users/laurenc82/Downloads/Clubs/Percentage Project/Percentage Project Website/src/components/PersonCard.js",["99"],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":81,"column":11,"nodeType":"106","endLine":81,"endColumn":46},{"ruleId":"104","severity":1,"m